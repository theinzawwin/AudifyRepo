<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - IRD</title>
    <link rel="stylesheet" href="~/mini/css/font-awesome.min.css" />
    <environment include="Development">
        <!-- Bootstrap 4.0-->
        <link rel="stylesheet" href="~/mini/css/bootstrap.css" />
        <link rel="stylesheet" href="~/mini/css/master_style.css" />
        <link rel="stylesheet" href="~/mini/css/fullcalendar.min.css" />
        <link rel="stylesheet" href="~/mini/css/_all-skins.css" />
    </environment>

    <environment include="Production">
        <!-- Bootstrap 4.0-->
        <link rel="stylesheet" href="~/mini/css/bootstrap.css" />
        <link rel="stylesheet" href="~/mini/css/master_style.css" />
        <link rel="stylesheet" href="~/mini/css/fullcalendar.min.css" />
        <link rel="stylesheet" href="~/mini/css/_all-skins.css" />
    </environment>

</head>
<body class="hold-transition login-page">

    @RenderBody()



    @RenderSection("Scripts", required: false)


    <environment include="Development">
        <script src="~/mini/js/jquery.min.js"></script>
        <script src="~/mini/js/jquery.dataTables.min.js"></script>
        <script src="~/mini/js/dataTables.bootstrap.min.js"></script>
        <script src="~/mini/js/popper.min.js"></script>
        <script src="~/mini/js/bootstrap.min.js"></script>
        <script src="~/mini/js/jquery.slimscroll.min.js"></script>
        <script src="~/mini/js/fastclick.js"></script>
        <script src="~/mini/js/template.js"></script>
        <script src="~/mini/js/demo.js"></script>
        <script src="~/mini/js/moment.js"></script>
        <script src="~/mini/js/fullcalendar.min.js"></script>
    </environment>
    <environment include="Production">

        <!-- jQuery 3 -->
        <script src="~/js/jquery.min.js"></script>

        <!-- popper -->
        <script src="~/js/popper.min.js"></script>

        <!-- Bootstrap 4.0-->
        <script src="~/js/bootstrap.min.js"></script>
    </environment>

</body>
</html>